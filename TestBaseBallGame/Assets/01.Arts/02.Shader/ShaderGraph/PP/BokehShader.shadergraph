    "m_FunctionBody": "// Shader Graph Custom Function 전용 HLSL (URP/BIRP 공용)\r\n// N x N 컨볼루션 (N은 홀수, 3/5/7/9/...)\r\n// 입력 커널은 '커널 텍스처'의 R 채널에 저장한다고 가정\r\n\r\ninline float2 Nani_ClampUV(float2 uv, float2 texel)\r\n{\r\n    // 경계는 clamp. 필요하면 mirror 등으로 바꿔도 됨.\r\n    return clamp(uv, texel * 0.5, 1.0 - texel * 0.5);\r\n}\r\n\r\n// 커널 합을 자동 정규화하고 싶으면 useAutoNormalize = 1 로 주고,\r\n// scale 은 1.0 으로 두면 됨. (수동 정규화면 useAutoNormalize = 0, scale=1/sum)\r\nvoid Nani_ConvolutionKernelTex(\r\n    Texture2D SourceTex, SamplerState SourceSamp,\r\n    float2 UV, float2 TexelSize, // 1/width, 1/height (SourceTex 기준)\r\n    Texture2D KernelTex, SamplerState KernelSamp, // NxN, R채널에 가중치\r\n    float KernelSize, float Scale, float Bias,\r\n    bool useAutoNormalize, // 0 or 1\r\n    out float4 OutColor)   // RGBA\r\n{\r\n    // 안전장치: 홀수 아니면 강제로 홀수화\r\n    if (((int) KernelSize & 1) == 0)\r\n        (int) KernelSize = KernelSize + 1;\r\n    int halfK = (int) KernelSize / 2;\r\n\r\n    // 커널 텍스처 크기\r\n    float2 kSize;\r\n    KernelTex.GetDimensions(kSize.x, kSize.y);\r\n    float2 kTexel = 1.0 / kSize;\r\n\r\n    float3 acc = 0;\r\n    float wsum = 0;\r\n\r\n    // 동적 루프 (모바일에서도 보통 OK. 아주 큰 커널은 주의)\r\n    [loop]\r\n    for (int j = -halfK; j <= halfK; j++)\r\n    {\r\n        [loop]\r\n        for (int i = -halfK; i <= halfK; i++)\r\n        {\r\n            float2 suv = Nani_ClampUV(UV + TexelSize * float2(i, j), TexelSize);\r\n\r\n            // 커널 텍스처에서 (i,j) 가중치 샘플\r\n            float2 kUV = (float2(i + halfK, j + halfK) + 0.5) * kTexel;\r\n            float w = KernelTex.Sample(KernelSamp, kUV).r;\r\n\r\n            float3 col = SourceTex.Sample(SourceSamp, suv).rgb;\r\n            acc += col * w;\r\n            wsum += w;\r\n        }\r\n    }\r\n\r\n    float norm = (useAutoNormalize != 0) ? (1.0 / max(wsum, 1e-6)) : Scale;\r\n    float3 rgb = acc * norm + Bias;\r\n    float a = SourceTex.Sample(SourceSamp, UV).a; // 알파는 중앙/원본 유지\r\n    OutColor = float4(rgb, a);\r\n}\r\n\r\n// (옵션) 대형 가우시안에 권장: 1D 분리형 (가로/세로 두 번)\r\n// Dir=(1,0) -> 중간결과 -> Dir=(0,1)\r\nfloat Nani_GetWeight(int idx, float4 W0, float4 W1, float4 W2, float4 W3)\r\n{\r\n    if (idx < 4)\r\n        return W0[idx];\r\n    if (idx < 8)\r\n        return W1[idx - 4];\r\n    if (idx < 12)\r\n        return W2[idx - 8];\r\n    return W3[idx - 12];\r\n}\r\n\r\nvoid Nani_Convolution1D(\r\n    Texture2D SourceTex, SamplerState SourceSamp,\r\n    float2 UV, float2 TexelSize,\r\n    float2 Dir, int Len, // Len 최대 16 예시\r\n    float4 W0, float4 W1, float4 W2, float4 W3,\r\n    out float4 OutColor)\r\n{\r\n    float3 acc = 0;\r\n    float wsum = 0;\r\n\r\n    [loop]\r\n    for (int k = 0; k < Len; k++)\r\n    {\r\n        float w = Nani_GetWeight(k, W0, W1, W2, W3);\r\n        float2 o = (k - (Len - 1) * 0.5) * Dir * TexelSize;\r\n        float2 suv = Nani_ClampUV(UV + o, TexelSize);\r\n        float3 c = SourceTex.Sample(SourceSamp, suv).rgb;\r\n        acc += c * w;\r\n        wsum += w;\r\n    }\r\n\r\n    float3 rgb = acc / max(wsum, 1e-6);\r\n    float a = SourceTex.Sample(SourceSamp, UV).a;\r\n    OutColor = float4(rgb, a);\r\n}\r\n"
}

